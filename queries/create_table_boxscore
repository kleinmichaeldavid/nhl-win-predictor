CREATE TABLE IF NOT EXISTS boxscore (
    game_id CHAR(10),
    game_type INT,
    season INT,
    datetime DATETIME,
    venue_name VARCHAR,
    venue_link VARCHAR,
    away_id INT,
    away_franchise_id INT,
    away_name VARCHAR,
    home_id INT,
    home_franchise_id INT,
    home_name VARCHAR,
    winner CHAR(4),
    shootout INT,
    away_goals INT,
    away_pim INT,
    away_shots INT,
    away_pp_goals INT,
    away_pp_attempts INT,
    away_fo_percent FLOAT,
    away_blocks INT,
    away_takeaways INT,
    away_giveaways INT,
    away_hits INT,
    home_goals INT,
    home_pim INT,
    home_shots INT,
    home_pp_goals INT,
    home_pp_attempts INT,
    home_fo_percent FLOAT,
    home_blocks INT,
    home_takeaways INT,
    home_giveaways INT,
    home_hits INT,
    UNIQUE(game_id)
);