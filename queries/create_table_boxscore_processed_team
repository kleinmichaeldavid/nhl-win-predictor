CREATE TABLE IF NOT EXISTS boxscore_processed_team (
    game_id CHAR(10),
    datetime DATETIME,
    season INT,
    franchise_id INT,
    games_played_before INT,
    games_played_after INT,
    win INT,
    wins_after INT,
    wins_before INT,
    losses_before INT,
    losses_after INT,
    shots_for INT,
    shots_against INT,
    shots_for_after INT,
    shots_against_after INT,
    shots_for_before INT,
    shots_against_before INT,
    goals_for INT,
    goals_against INT,
    goals_for_after INT,
    goals_against_after INT,
    goals_for_before INT,
    goals_against_before INT,
    pim_for INT,
    pim_against INT,
    pim_for_after INT,
    pim_against_after INT,
    pim_for_before INT,
    pim_against_before INT,
    pp_goals_for INT,
    pp_goals_against INT,
    pp_goals_for_after INT,
    pp_goals_against_after INT,
    pp_goals_for_before INT,
    pp_goals_against_before INT,
    pp_attempts_for INT,
    pp_attempts_against INT,
    pp_attempts_for_after INT,
    pp_attempts_against_after INT,
    pp_attempts_for_before INT,
    pp_attempts_against_before INT,
    fo_percent_for FLOAT,
    fo_percent_against FLOAT,
    fo_percent_for_after FLOAT,
    fo_percent_against_after FLOAT,
    fo_percent_for_before FLOAT,
    fo_percent_against_before FLOAT,
    blocks_for INT,
    blocks_against INT,
    blocks_for_after INT,
    blocks_against_after INT,
    blocks_for_before INT,
    blocks_against_before INT,
    takeaways_for INT,
    takeaways_against INT,
    takeaways_for_after INT,
    takeaways_against_after INT,
    takeaways_for_before INT,
    takeaways_against_before INT,
    giveaways_for INT,
    giveaways_against INT,
    giveaways_for_after INT,
    giveaways_against_after INT,
    giveaways_for_before INT,
    giveaways_against_before INT,
    hits_for INT,
    hits_against INT,
    hits_for_after INT,
    hits_against_after INT,
    hits_for_before INT,
    hits_against_before INT,
    UNIQUE(game_id, franchise_id)
);