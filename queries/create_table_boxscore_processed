CREATE TABLE IF NOT EXISTS boxscore_processed (
    game_id CHAR(10),
    game_type INT,
    season INT,
    datetime DATETIME,
    venue_name VARCHAR,
    venue_link VARCHAR,
    away_id INT,
    away_franchise_id INT,
    away_name VARCHAR,
    home_id INT,
    home_franchise_id INT,
    home_name VARCHAR,
    winner CHAR(4),
    shootout INT,
    away_goals INT,
    away_pim INT,
    away_shots INT,
    away_pp_goals INT,
    away_pp_attempts INT,
    away_fo_percent FLOAT,
    away_blocks INT,
    away_takeaways INT,
    away_giveaways INT,
    away_hits INT,
    home_goals INT,
    home_pim INT,
    home_shots INT,
    home_pp_goals INT,
    home_pp_attempts INT,
    home_fo_percent FLOAT,
    home_blocks INT,
    home_takeaways INT,
    home_giveaways INT,
    home_hits INT,
    home_season_games_played INT,
    away_season_games_played INT,
    home_season_wins INT,
    away_season_wins INT,
    home_season_losses INT,
    away_season_losses INT,
    home_season_shots_for INT,
    away_season_shots_for INT,
    home_season_shots_against INT,
    away_season_shots_against INT,
    home_season_goals_for INT,
    away_season_goals_for INT,
    home_season_goals_against INT,
    away_season_goals_against INT,
    home_season_pim_for INT,
    away_season_pim_for INT,
    home_season_pim_against INT,
    away_season_pim_against INT,
    home_season_pp_goals_for INT,
    away_season_pp_goals_for INT,
    home_season_pp_goals_against INT,
    away_season_pp_goals_against INT,
    home_season_pp_attempts_for INT,
    away_season_pp_attempts_for INT,
    home_season_pp_attempts_against INT,
    away_season_pp_attempts_against INT,
    home_season_fo_percent_for FLOAT,
    away_season_fo_percent_for FLOAT,
    home_season_fo_percent_against FLOAT,
    away_season_fo_percent_against FLOAT,
    home_season_blocks_for INT,
    away_season_blocks_for INT,
    home_season_blocks_against INT,
    away_season_blocks_against INT,
    home_season_takeaways_for INT,
    away_season_takeaways_for INT,
    home_season_takeaways_against INT,
    away_season_takeaways_against INT,
    home_season_giveaways_for INT,
    away_season_giveaways_for INT,
    home_season_giveaways_against INT,
    away_season_giveaways_against INT,
    home_season_hits_for INT,
    away_season_hits_for INT,
    home_season_hits_against INT,
    away_season_hits_against INT,
    UNIQUE(game_id)
);